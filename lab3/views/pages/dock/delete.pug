extends ../../layout

block content
  div(class='col-md-6')
    form(id='dock-delete-form' name='dock-delete-form' class='form-horizontal' method="POST" action='')
      input(id='id' type="hidden" name='id' value=dock.id class='form-control')
      div(class='form-group')
        div(class='col-md-12') Number: #{dock.number};
      div(class='form-group')
        div(class='col-md-12')
          button(type='submit' class='btn btn-primary') Delete
